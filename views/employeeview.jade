
head
    script(defer src="/js/employeeView.js")
    link(rel="stylesheet", href="/css/employeeview.css")
    
body
    .modal.fade#viewmodal(tabindex="-1" role="dialog" aria-labelledby="modalTitleId" aria-hidden="true")
            .modal-dialog.modal-fullscreen(role="document")
               .modal-content
                    .modal-header
                        h5.modal-title#modalTitleId
                            Modal Employee Detail
                        button(type="button" data-bs-dismiss="modal" aria-label="Close").btn-close
                    .modal-body    
                        .row 
                            .col-lg-3.mt-4
                                table.employeeimgtable
                                    tr
                                        .employeeimg
                                            img#employeeprofile(src="data:image/jpeg;base64," height="250px" width="200px")
                                    tr 
                                        th Status
                                        td#viewstatus 
                                    tr 
                                        th ID
                                        td#viewid  
                            .col-lg-5
                                table.employeetableview
                                    tr 
                                    th Name 
                                    td#viewname
                                    tr    
                                        th Email 
                                        td#viewemail
                                    tr
                                        th Branch 
                                        td#viewbranch
                                    tr 
                                        th Address 
                                        td#viewaddress
                                    tr 
                                        th Contact 
                                        td#viewcontact
                            .col-lg-3   
                                table.employeetableview     
                                    tr
                                        th Joindate 
                                        td#viewjoinDate
                                    tr 
                                        th EndDate 
                                        td#viewendDate
                                    tr 
                                        th  Salary
                                        td#viewsalary
                                    tr 
                                        th  Department 
                                        td#viewdepartment
                                    tr 
                                        th Last updated At 
                                        td#viewupdated
                    .modal-footer
                        button.btn.btn-danger(aria-label="Close" data-bs-dismiss="modal") close    
                                    
                                        